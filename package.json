{
  "name": "rehype-quotebacks",
  "version": "0.0.1",
  "description": "rehype plugin to render blockquotes as quotebacks",
  "license": "MIT",
  "keywords": [
    "markdown",
    "mdast",
    "plugin",
    "rehype",
    "rehype-plugin",
    "unified"
  ],
  "repository": "github:stigi/rehype-quotebacks",
  "bugs": "https://github.com/stigi/rehype-quotebacks/issues",
  "author": "Ullrich Sch√§fer <hi+github@ullrich.is> (https://ullrich.is)",
  "contributors": [],
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/mdast": "^4.0.0",
    "hast-util-whitespace": "^3.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^6.1.2",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.0",
    "xo": "^0.58.0"
  },
  "scripts": {
    "test": "xo && ava",
    "test:watch": "ava --watch"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "capitalized-comments": "off"
    }
